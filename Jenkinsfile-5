pipeline {
	  agent { dockerfile true }
	  stages {
	    stage('Test') {
	      steps {
	        sh '''
	          python test.py
	        '''
	      }
	    }
	  }
	}
pipeline {
	  agent none
	  stages {
	    stage('download') {
	      agent any
	      steps {
	        sh 'mvn --version'
	      }
	    }
	    stage('test') {
	      agent { dockerfile true}
	      steps {
	        sh 'python test.py'
	      }
	    }
	  }
	}

