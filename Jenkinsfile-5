pipeline {
	  agent none
	  stages {
	    stage('download') {
	      agent any
	      steps {
	        sh 'wget https://raw.githubusercontent.com/zecko8/jenkins-lab1/main/tesp.py'
		sh 'pwd'
		sh 'ls'
	      }
	    }
	    stage('test') {
	      agent { dockerfile true}
	      steps {
	        sh 'python test.py'
	      }
	    }
	  }
	}

